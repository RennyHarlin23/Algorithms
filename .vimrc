source $VIMRUNTIME/vimrc_example.vim
syntax on
set cb=unnamed
set tabstop=2
set shiftwidth=2
set gfn=Fixedsys:h12
set expandtab
set noerrorbells visualbell t_vb=
set laststatus=2
set swapfile
set dir=C:\Users\renny\Documents\.vim\temp
set undofile
set undodir=C:\Users\renny\Documents\.vim\undodir
cd C:\Users\renny\Documents\vimws
set ai
set si
set ci
set number
set hlsearch
set ruler
highlight Comment ctermfg=green
inoremap { {}<Left>
inoremap {<CR> {<CR>}<Esc>O
set shell=pwsh
